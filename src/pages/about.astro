---
import SectionWrapper from "../components/About/SectionWrapper.astro";
import Organization from "../components/About/Organization.astro";
import TimelineSection from "../components/About/Timeline/TimelineSection.astro";
import MainNoSideLayout from "../layouts/MainNoSideLayout.astro";
import { getAllHistory } from "../apis/history";

let historyData = null;
historyData = await getAllHistory();
---

<MainNoSideLayout title="ABOUT">
  <SectionWrapper title="Organization" description="">
    <Organization />
  </SectionWrapper>

  <TimelineSection historyData={historyData} />
</MainNoSideLayout>
